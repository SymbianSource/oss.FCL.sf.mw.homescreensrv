//  STRUCTURE DEFINITIONS

// -----------------------------------------------------------------------------
// DEFAULT_PROFILES
// Default profiles
// -----------------------------------------------------------------------------
//
STRUCT DEFAULT_PROFILES
    {
    STRUCT items[];
    }

// -----------------------------------------------------------------------------
// PROFILE
// profile default name and ID
// -----------------------------------------------------------------------------
//
STRUCT PROFILE
    {
    LONG id = 0;
    LTEXT name = "";
    LTEXT shortname = "";
    LTEXT naviname = "";
    LTEXT titlename = "";
    }

// -----------------------------------------------------------------------------
// PROFILE_SETTINGS
// Profile settings
// -----------------------------------------------------------------------------
//
STRUCT PROFILE_SETTINGS
    {
    STRUCT items[];
    }

// -----------------------------------------------------------------------------
// PROFILE SETTING
// profile setting for dynamic profiles
// -----------------------------------------------------------------------------
//
STRUCT PROFILE_SETTING
    {
    LONG id = 0;
    LONG key_type = 0;
    }

// End of File
