//----------------------------------------------------
// r_scutsettings_menupane
// Options menu.
//----------------------------------------------------
//
RESOURCE MENU_PANE r_scutsettings_menupane
{
    items =
    {
        MENU_ITEM
        {
            command = EAiScutSettingsCmdChange;
            txt     = qtn_options_change;
        }

#ifdef __SERIES60_HELP
        ,
        MENU_ITEM
        {
            command = EAknCmdHelp;
            txt     = qtn_options_help;
        }
