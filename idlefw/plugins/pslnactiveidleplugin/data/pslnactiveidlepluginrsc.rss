NAME    PSAI // 4 letter ID

#include <avkon.loc> // Avkon localized strings
#include <avkon.mbg>
#include <avkon.rsg>
#include <eikcore.rsg>
#include <pslncommon.rsg>
#include <uikon.rh>
#include <avkon.rh>
#include <eikon.rh>

#include <pslnactiveidle3plugin.loc>
